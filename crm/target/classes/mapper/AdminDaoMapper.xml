<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinasofti.dao.AdminDao">
    <insert id="insert" parameterType="com.chinasofti.po.Admin">
        INSERT INTO admin(adminId,adminAccount,adminPassword,adminName)
        VALUES (#{adminId},#{adminAccount},#{adminPassword},#{adminName})
    </insert>

</mapper>